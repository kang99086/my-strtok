#include <stdio.h>

void m_strtok(char *str1,char *const str2)
{
	while(*str1 != NULL){
		if(*str1 != *str2) {
			printf("%c",*str1);
		}
		else {
			printf("\n");
		}
		str1++;
	}
		printf("\n");
}

int main()
{
	char str1[100]="cprogramming is so hard";
	char str2[10]=" ";
	m_strtok(&str1,&str2);
}